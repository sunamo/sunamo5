<UserControl x:Class="desktop.Controls.ResultButtons"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:desktop.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel x:Name="sp" VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Horizontal">
            <StackPanel x:Name="spCustom" x:FieldModifier="public"></StackPanel>
            <Button x:Name="btnCopyToClipboard" Margin="10" Click="btnCopyToClipboard_Click"></Button>
            <Button x:Name="btnAllRight" Margin="10" Click="btnAllRight_Click"></Button>
        </StackPanel>
    </Grid>
</UserControl>
